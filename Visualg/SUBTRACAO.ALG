Algoritmo "subtracao"

Var

       n1, n2, subtrair : real

Inicio

       escreva("Digite um número: ")
         leia(n1)
         
       escreva("Digite outro número: ")
         leia(n2)
         
         
         subtrair <- n1 - n2
         
        escreval(subtrair)

       
       
       
Fimalgoritmo