Algoritmo "divisao"

Var

   divisao, n1, n2 : real

Inicio

        escreva("Digite o primeiro número: ")
          leia(n1)
          
        escreva("Digite o primeiro número: ")
          leia(n2)
          
          
          divisao <- n1 / n2
          
          escreval(divisao)


Fimalgoritmo