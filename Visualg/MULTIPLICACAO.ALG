Algoritmo "multiplicacao"

Var

   multiplicacao, n1, n2 : inteiro

Inicio

        escreva("Digite o primeiro número: ")
          leia(n1)
          
        escreva("Digite o primeiro número: ")
          leia(n2)
          
          
          multiplicacao <- n1 * n2
          
          escreval(multiplicacao)


Fimalgoritmo